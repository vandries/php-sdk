{
    "name": "musicstory/php-sdk",
    "description": "PHP SDK for Music Story API",
    "homepage": "http://developers.music-story.com/",
    "type": "library",
    "authors": [
        {
            "name": "Music Story",
            "email": "webmaster@music-story.com"
        }
    ],
    "support": {
        "email": "webmaster@music-story.com",
        "issues": "https://github.com/music-story/php-sdk/issues"
    },
    "require": {
        "monolog/monolog": "^1.18",
        "league/oauth1-client": "~1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2"
    },
    "autoload": {
        "psr-4": { "MusicStory\\SDK\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "MusicStory\\SDK\\Tests\\": "tests/" }
    }
}
